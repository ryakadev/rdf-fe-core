{
	"name": "@ryakadev/fe-core",
	"version": "3.0.0",
	"engines": {
		"node": ">=21.1.0"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"README.md"
	],
	"scripts": {
		"prepare": "husky",
		"dev": "next dev --turbopack",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"lint:fix": "next lint --fix",
		"format": "prettier . --write",
		"test": "jest --passWithNoTests",
		"test:watch": "jest --watch",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"build-npm": "rm -rf dist && tsup",
		"build-npm:check": "npm run build-npm && npm pack --dry-run",
		"prepublishOnly": "npm run build-npm && node scripts/generate-exports.mjs",
		"build-npm:publish": "npm publish --access public"
	},
	"dependencies": {
		"@hookform/resolvers": "^4.1.3",
		"@radix-ui/react-label": "^2.1.2",
		"@radix-ui/react-slot": "^1.1.2",
		"bcryptjs": "^3.0.2",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"jose": "^6.0.10",
		"lodash": "^4.17.21",
		"lucide-react": "^0.469.0",
		"motion": "^11.15.0",
		"react-dom": "^19.0.0",
		"react-hook-form": "^7.54.2",
		"tailwind-merge": "^2.5.5",
		"zod": "^3.24.2"
	},
	"peerDependencies": {
		"next": "15.1.2",
		"react": "^19.0.0",
		"react-dom": "^19.0.0"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^3.2.4",
		"@commitlint/cli": "^19.7.1",
		"@commitlint/config-conventional": "^19.7.1",
		"@eslint/eslintrc": "^3",
		"@storybook/addon-essentials": "^8.5.6",
		"@storybook/addon-interactions": "^8.5.6",
		"@storybook/addon-onboarding": "^8.5.6",
		"@storybook/blocks": "^8.5.6",
		"@storybook/nextjs": "^8.5.6",
		"@storybook/react": "^8.5.6",
		"@storybook/test": "^8.5.6",
		"@tailwindcss/postcss": "^4.0.0",
		"@tailwindcss/typography": "^0.5.16",
		"@testing-library/dom": "^10.4.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.1.0",
		"@types/jest": "^29.5.14",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"eslint": "^9",
		"eslint-config-next": "15.1.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-storybook": "^0.11.3",
		"husky": "^9.1.7",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"postcss": "^8",
		"prettier": "^3.4.2",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"storybook": "^8.5.6",
		"tailwindcss": "^4.0.0",
		"ts-node": "^10.9.2",
		"tsup": "^8.4.0",
		"typescript": "^5"
	},
	"eslintConfig": {
		"extends": [
			"plugin:storybook/recommended"
		]
	},
	"exports": {
		"./components/blocks/*": {
			"types": "./dist/components/blocks/*.d.mts",
			"import": "./dist/components/blocks/*.mjs",
			"require": "./dist/components/blocks/*.js"
		},
		"./components/ui/*": {
			"types": "./dist/components/ui/*.d.mts",
			"import": "./dist/components/ui/*.mjs",
			"require": "./dist/components/ui/*.js"
		},
		"./lib/*": {
			"types": "./dist/lib/*.d.mts",
			"import": "./dist/lib/*.mjs",
			"require": "./dist/lib/*.js"
		}
	}
}
